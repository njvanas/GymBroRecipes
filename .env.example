# Base URL where your app runs
# Example: http://localhost:3000 for local dev
APP_URL=http://localhost:3000

# Supabase configuration for the frontend
# URL of your Supabase instance
VITE_SUPABASE_URL=
# Public anon key used by the client
VITE_SUPABASE_ANON_KEY=

# Supabase configuration for backend functions
# Same URL as above
SUPABASE_URL=
# Service role key, keep this secret
SUPABASE_SERVICE_KEY=

# Stripe billing configuration
# Secret key for Stripe API requests
STRIPE_SECRET_KEY=
# Price ID for the checkout session
STRIPE_PRICE_ID=

# Redirect URLs after successful or canceled payment
SUCCESS_URL=http://localhost/success
CANCEL_URL=http://localhost/cancel
